mkdir -p /var/www/vhosts/dev.miguelgonzalezgomez.com/var/cache
mkdir -p /var/www/vhosts/dev.miguelgonzalezgomez.com/var/logs
mkdir -p /var/www/vhosts/dev.miguelgonzalezgomez.com/var/sessions

chown ec2-user /var/www/vhosts/dev.miguelgonzalezgomez.com/var/logs
chown ec2-user /var/www/vhosts/dev.miguelgonzalezgomez.com/var/cache

chgrp www /var/www/vhosts/dev.miguelgonzalezgomez.com/var/logs
chgrp www /var/www/vhosts/dev.miguelgonzalezgomez.com/var/cache

chmod -R 770 /var/www/vhosts/dev.miguelgonzalezgomez.com/var/cache
chmod -R 770 /var/www/vhosts/dev.miguelgonzalezgomez.com/var/logs

chown -R ec2-user /var/www/vhosts/dev.miguelgonzalezgomez.com/bin

cp /home/ec2-user/app-paramaters/parameters.yml /var/www/vhosts/dev.miguelgonzalezgomez.com/app/config/parameters.yml

composer install --no-dev --no-scripts --no-interaction --optimize-autoloader -d /var/www/vhosts/dev.miguelgonzalezgomez.com/